</span></span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">import 'package:cloud_firestore/cloud_firestore.dart';
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">class PostModel {
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">  final String postId;
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">  final String ownerId;
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">  final String username;
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">  final String imageUrl;
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">  final String caption;
