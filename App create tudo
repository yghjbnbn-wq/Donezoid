</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">                    const url = 'https://pixelhive.com/privacy';
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">                    if (await canLaunchUrl(Uri.parse(url))) {
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">                      await launchUrl(Uri.parse(url));
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">                    } else {
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">                      Helpers.showSnackBar(context, 'Could not open privacy policy', isError: true);
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">                    }
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">                  },
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">                ),
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">                const Divider(),
</span><span class="token token" style="opacity: 1; font-size: inherit; line-height: 1.42857; color: rgb(181, 189, 104); background-color: transparent; flex-shrink: 0; padding: 8px; text-align: right; user-select: none;">                _buildDrawerItem(
